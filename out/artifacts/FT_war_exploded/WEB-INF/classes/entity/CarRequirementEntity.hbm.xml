<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CarRequirementEntity" table="car_requirement" schema="ibm_icbc">
        <id name="carRequirementId" column="car_requirement_id">
            <generator class="uuid"/>
        </id>
        <property name="carRequirementUserId" column="car_requirement_user_id"/>
        <property name="carRequirementDriverSex" column="car_requirement_driver_sex"/>
        <property name="carRequirementPassengerSex" column="car_requirement_passenger_sex"/>
        <property name="carRequirementMaxPassenger" column="car_requirement_max_passenger"/>
    </class>
</hibernate-mapping>