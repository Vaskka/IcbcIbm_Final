<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ItemEntity" table="item" schema="ibm_icbc">
        <id name="itemId" column="item_id">
            <generator class="uuid"/>
        </id>
        <property name="itemFromBusinessId" column="item_from_business_id"/>
        <property name="itemValue" column="item_value"/>
        <property name="itemImage" column="item_image"/>
        <property name="itemName" column="item_name"/>
        <property name="itemCount" column="item_count"/>
    </class>
</hibernate-mapping>